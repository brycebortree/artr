<h1> The Gallery </h1>
  <div class="art col-md-offset-2 col-md-8 col-md-offset-2">
    <% arts.forEach(function(art) { %>
      <div>
        <img src="<%= art.flickrURL %>">
      </div>
      <div class="card">
        <p><%= art.twitUser %></p>
        <p><%= art.query %>, 2016</p>
        <p> The Artr </p>
        <p><%= art.tweetStatement %></p>
      </div>
      <hr>
<!--       <button class="addFav">Favorite </button>-->
    <% }); %>
  </div>
  
